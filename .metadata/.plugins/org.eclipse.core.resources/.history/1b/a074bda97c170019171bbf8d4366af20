
public class ImmutableStack<T> {
	
	private T item;
	private int stackSize;
	private T[] previousStack;
	

	public ImmutableStack() {
		item = null;
		stackSize = 0;
	}
	
	@SuppressWarnings("unchecked")
	public ImmutableStack(T item, int stackSize) {
		this.item = item;
		this.stackSize = stackSize;
		previousStack = (T[]) new Object[stackSize];
	}
	
	public int count() {
		return stackSize;
	}
	
	public ImmutableStack<T> pop() {
		return null;
	}
	
	public ImmutableStack<T> push(T t) throws IllegalArgumentException {
		return new ImmutableStack<T>(t, stackSize + 1);
	}
	
	public T peek() {
		return item;
	}

	public static void main(String[] args) {
		// test class here
		System.out.println("Hello, World!");
	}
}
