
public class ImmutableStack<T> {
	
	private T[] stack;
	private int currentIndex;

	@SuppressWarnings("unchecked")
	public ImmutableStack(int capacity) {
		stack = (T[]) new Object[capacity];
		currentIndex = 0;
	}
	
	public T pop() {
		return null;
	}
	
	public void push(T t) throws IllegalArgumentException {
		return;
	}
	
	public T peek() {
		return null;
	}

}
