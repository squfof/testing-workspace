
public class FizzBuzz {
	
	public String getResult(int n) {
		String r = ""; // stores return value
		
		if (n % 3 == 0)
			r += "Fizz";
		else if (n % 5 == 0)
			r += "Buzz";
		else
			r += String.valueOf(n);
		
		return String.valueOf(r);
	}

	public static void main(String[] args) {
		// test class here
		FizzBuzz fb = new FizzBuzz();
		
		for (int i = 1; i < 31; i++)
			System.out.println(fb.getResult(i));
	}

}
